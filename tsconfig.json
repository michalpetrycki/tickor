{
    "compilerOptions": {
        "target": "es2016",
        "module": "commonjs",
        "baseUrl": "./src",
        "paths": {
            "@/resources/*" : ["resources/*"],
            "@/utils/*" : ["utils/*"],
            "@/middleware/*" : ["middleware/*"]
        },
        "resolveJsonModule": true,
        "sourceMap": true,
        "outDir": "dist",
        "esModuleInterop": true,"forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "lib": ["DOM"]
    }
  }
  